fn square(num:i32) -> i32 {
    return num*num;
}

fn fib(n:i32) -> i32 {
    if n==0  {
        return 1;
    }else if n<1 {
        return 1;
    }
    else {
        return fib(n-1) +fib(n-2);
    }
}

fn main() {
    

    let i =0;

    while(i<10) {
         i = i+1;
         printf("%ld\n",i);
    }

    for(let i=0;i<20;i = i+2) {
        printf("%ld\n",i);
    }

    let i = fib(10);

    printf("%ld\n",i);
    // 

}



